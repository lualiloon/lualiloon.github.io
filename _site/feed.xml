<?xml version="1.0" encoding="utf-8"?><feed xmlns="http://www.w3.org/2005/Atom" ><generator uri="https://jekyllrb.com/" version="4.3.1">Jekyll</generator><link href="http://localhost:4000/feed.xml" rel="self" type="application/atom+xml" /><link href="http://localhost:4000/" rel="alternate" type="text/html" /><updated>2023-05-28T15:12:17-04:00</updated><id>http://localhost:4000/feed.xml</id><entry><title type="html">First Post</title><link href="http://localhost:4000/2023/05/28/first-post.html" rel="alternate" type="text/html" title="First Post" /><published>2023-05-28T14:57:46-04:00</published><updated>2023-05-28T14:57:46-04:00</updated><id>http://localhost:4000/2023/05/28/first-post</id><content type="html" xml:base="http://localhost:4000/2023/05/28/first-post.html"><![CDATA[<script>
function removeStyleSheet(sheet) {
  sheet.ownerNode.parentNode.removeChild(sheet.ownerNode);
}

function removeAllSheets() {
  while (document.styleSheets.length > 0) {
    removeStyleSheet(document.styleSheets[0]);
  }
}

function addStyleSheet(sheet) {
  var link = document.createElement('link');
  link.rel = "stylesheet";
  link.type = "text/css";
  link.href = sheet;
  document.head.appendChild(link);
}

function swapStyleSheets(newSheet) {
  removeAllSheets();
  addStyleSheet(newSheet);
}
</script>

<p><button onclick="swapStyleSheets('/assets/css/styles.css')">First Style</button></p>

<p><button onclick="swapStyleSheets('/assets/css/alternative-styles.css')">Second Style</button></p>]]></content><author><name></name></author><summary type="html"><![CDATA[]]></summary></entry></feed>